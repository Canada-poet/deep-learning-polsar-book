@ARTICLE{lee2004classification,
author={Jong-Sen Lee and Grunes, M.R. and Pottier, E. and Ferro-Famil, L.},
journal={Geoscience and Remote Sensing, IEEE Transactions on},
title={Unsupervised terrain classification preserving polarimetric scattering characteristics},
year={2004},
